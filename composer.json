  "type": "project",
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "photogabble/tuppence": "^1.1",
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5",
    "symfony/console": "^4.1",
    "adbario/php-dot-notation": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
